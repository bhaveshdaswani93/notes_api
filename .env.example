# Environment example for Notes API
# Copy this to `.env` and fill values before running the app.

# Scalekit configuration
# Get these from your Scalekit Dashboard: https://app.scalekit.com
SCALEKIT_ENV_URL=https://yourtenant.scalekit.com
SCALEKIT_CLIENT_ID=your_client_id_here
SCALEKIT_CLIENT_SECRET=your_client_secret_here

# Callback and redirect URLs (must match Scalekit dashboard settings)
SCALEKIT_REDIRECT_URI=http://localhost:3000/auth/callback
SCALEKIT_POST_LOGOUT_REDIRECT_URI=http://localhost:3000

# Frontend URL for CORS (Next.js or other frontend)
FRONTEND_URL=http://localhost:3001

# App
PORT=3000
